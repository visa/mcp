{
  "name": "@visa/token-manager",
  "version": "1.0.0",
  "description": "Visa token generation and management for MCP authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.js,.mjs --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "visa",
    "token",
    "authentication",
    "jwe",
    "mcp"
  ],
  "dependencies": {
    "jose": "^5.9.6",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
