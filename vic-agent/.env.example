
# LangSmith Tracing (optional - for debugging and monitoring)
LANGCHAIN_TRACING_V2=true
LANGSMITH_ENDPOINT=https://your-langsmith-endpoint
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=your-project-name

# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_BASE_URL=if_needed
MODEL_TOP_P=0.9
MODEL_TOP_K=40

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=if_needed

# Visa MCP Client Configuration
# Copy this file to .env and fill in your actual values

# Base URL for Visa MCP server
# For sandbox: https://sandbox.mcp.visa.com
VISA_MCP_BASE_URL=https://sandbox.mcp.visa.com

# Base URL for Visa VIC API
# For sandbox: https://sandbox.api.visa.com
VISA_API_BASE_URL=https://sandbox.api.visa.com

# Base URL for Visa VTS API (different from VIC)
# For cert/sandbox: https://cert.api.visa.com
VISA_VTS_API_BASE_URL=https://cert.api.visa.com

# API Mode Configuration
# Set to 'true' to use direct API calls instead of MCP server
USE_DIRECT_API=true

# Visa Intelligence Commerce (VIC) API credentials
# Obtained during Visa Intelligent Commerce onboarding process
VISA_VIC_API_KEY=your_vic_api_key_here
VISA_VIC_API_KEY_SS=your_vic_api_key_shared_secret_here

# External client identification
# Obtained during Visa Intelligent Commerce onboarding process
VISA_EXTERNAL_CLIENT_ID=your_external_client_id_here
VISA_EXTERNAL_APP_ID=your_external_app_id_here
VISA_AUTHORIZATION=your_authorization_optional
VISA_RELATIONSHIP_ID=your_relationship_id_optional

# Visa Token Service (VTS) API credentials
# Obtained during VTS onboarding process
VISA_VTS_API_KEY=your_vts_api_key_here
VISA_VTS_API_KEY_SS=your_vts_api_key_shared_secret_here

# Message Level Encryption (MLE) certificates
# Generate these by following: https://developer.visa.com/pages/encryption_guide
# These should be PEM-formatted strings (with \n for newlines)
VISA_MLE_SERVER_CERT="-----BEGIN CERTIFICATE-----...-----END CERTIFICATE-----"
VISA_MLE_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----...-----END RSA PRIVATE KEY-----"
VISA_KEY_ID=your_key_id_here

# Your JWT signing private key (NOT provided by Visa)
# Generate your own RSA key pair - this key is used to sign JWT before encryption.
# Must be a PKCS#8 PEM key
USER_SIGNING_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----...-----END PRIVATE KEY-----"

# Workflow Configuration
# Consumer identifier - keep consistent across all workflow executions
VISA_CONSUMER_ID=your_consumer_id_here

# Token Requestor Configuration
TR_ID=your_wallet_account_id_here
TR_APPID=your_app_id_here
TR_HASHSECRET=your_hash_secret_here
TR_ENCSECRET=your_encryption_secret_here
TR_ENCAPIKEY=your_encryption_api_key_here