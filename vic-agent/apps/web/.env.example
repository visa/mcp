
# Next.js Web App Configuration

# Agent Backend API Endpoint
# URL where the LangGraph agent server is running
NEXT_PUBLIC_API_URL=http://localhost:2024

# Agent Assistant Identifier
# ID used to identify the agent in API requests
NEXT_PUBLIC_ASSISTANT_ID=vic-agent

# Available LLM Models
# Comma-separated list of models that users can select in the UI
# Format: provider/model-name (e.g., anthropic/claude-sonnet-4-5-20250929)
NEXT_PUBLIC_AVAILABLE_MODELS=anthropic/claude-sonnet-4-5-20250929,openai/gpt-5

# Default LLM Model
# Model selected by default when the application starts
NEXT_PUBLIC_DEFAULT_MODEL=anthropic/claude-sonnet-4-5-20250929

# VTS Authentication Configuration
# Base URL for Visa Token Service authentication
NEXT_PUBLIC_VTS_AUTH_BASE_URL=https://sbx.vts.auth.visa.com
# VTS API key for authentication
NEXT_PUBLIC_VTS_API_KEY=YOUR_API_KEY
# VTS external application ID
NEXT_PUBLIC_VTS_EXTERNAL_APP_ID=YOUR_APP_ID

